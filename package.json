{
  "name": "tiktok-shop-sdk",
  "version": "0.5.0",
  "description": "A TypeScript SDK for interacting with TikTok Shop OpenAPI.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsup",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "tiktok",
    "tiktok-shop",
    "sdk",
    "typescript",
    "api"
  ],
  "author": "Hasib Muharam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hsib19/tiktok-shop-sdk.git"
  },
  "homepage": "https://github.com/hsib19/tiktok-shop-sdk#readme",
  "bugs": {
    "url": "https://github.com/hsib19/tiktok-shop-sdk/issues"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.23",
    "@typescript-eslint/parser": "^8.33.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
    "publishConfig": {
    "access": "public"
  }
}
